{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/fundamental-pract/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/fundamental-pract/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/fundamental-pract",
  "projects": {
    "first-svc": {
      "type": "application",
      "root": "apps/first-svc",
      "entryFile": "main",
      "sourceRoot": "apps/first-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/first-svc/tsconfig.app.json"
      }
    },
    "my-app": {
      "type": "application",
      "root": "apps/my-app",
      "entryFile": "main",
      "sourceRoot": "apps/my-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/my-app/tsconfig.app.json"
      }
    },
    "second-svc": {
      "type": "application",
      "root": "apps/second-svc",
      "entryFile": "main",
      "sourceRoot": "apps/second-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/second-svc/tsconfig.app.json"
      }
    },
    "test-app": {
      "type": "application",
      "root": "apps/test-app",
      "entryFile": "main",
      "sourceRoot": "apps/test-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/test-app/tsconfig.app.json"
      }
    },
    "third-svc": {
      "type": "application",
      "root": "apps/third-svc",
      "entryFile": "main",
      "sourceRoot": "apps/third-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/third-svc/tsconfig.app.json"
      }
    }
  }
}